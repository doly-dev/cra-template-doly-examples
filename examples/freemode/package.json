{
  "name": "my-app2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "antd-mobile": "^2.3.4",
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "history": "^4.10.1",
    "rc-hooks": "^1.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.2",
    "store2": "^2.12.0"
  },
  "scripts": {
    "start": "nodemon --exec \"env-cmd -r ./config/env.js -e default craco start\"",
    "build": "env-cmd -r ./config/env.js -e default,prod craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "start:no-mock": "cross-env MOCK=none nodemon --exec \"env-cmd -r ./config/env.js -e default,dev craco start\"",
    "start:dev": "cross-env MOCK=none nodemon --exec \"env-cmd -r ./config/env.js -e default,dev craco start\"",
    "start:test": "cross-env MOCK=none nodemon --exec \"env-cmd -r ./config/env.js -e default,test craco start\"",
    "zip": "node ./config/zip.js",
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "commit": "cz",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cracoConfig": "config/config.js",
  "nodemonConfig": {
    "ignore": "config/zip.js",
    "watch": "config"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.css": "stylelint --syntax css",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,sass,less,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@craco/craco": "^6.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.24",
    "@types/loadable__component": "^5.13.4",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^12.20.25",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "@types/react-transition-group": "^4.4.2",
    "archiver": "^5.3.0",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.2",
    "craco-less": "^1.20.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "glob": "^7.1.7",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mkdirp": "^1.0.4",
    "mocker-api": "^2.9.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "react-scripts": "^4.0.3",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "typescript": "^4.4.3",
    "web-vitals": "^1.1.2",
    "webpackbar": "^5.0.0-3"
  }
}
